{
	"name": "jump-tag",
	"displayName": "Jump Tag",
	"description": "Navigate through your XML/HTML code faster",
	"author": "Yurii Semeniuk",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:jumpTag.prev",
		"onCommand:jumpTag.next"
	],
	"main": "./extension.js",
	"contributes": {
	  "commands": [
		{
		  "command": "jumpTag.prev",
		  "title": "Jump Tag - Previous"
		},
		{
		  "command": "jumpTag.next",
		  "title": "Jump Tag - Next"
		}
	  ],
	  "keybindings": [
		{
		  "mac": "cmd+alt+left",
		  "key": "ctrl+alt+left",
		  "command": "jumpTag.prev",
		  "when": "editorTextFocus"
		},
		{
		  "mac": "cmd+alt+right",
		  "key": "ctrl+alt+right",
		  "command": "jumpTag.next",
		  "when": "editorTextFocus"
		}
	  ]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
